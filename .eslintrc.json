{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": 0,
        "prefer-destructuring": 0,
        "import/extensions": 0,
        "import/prefer-default-export": 0,
        "eqeqeq": "off",
        "no-mixed-operators": "off",
        "no-console": "off",
        "no-case-declarations": "off",
        "no-inner-declarations": "off",
        "no-return-await": "off"
    }
}
